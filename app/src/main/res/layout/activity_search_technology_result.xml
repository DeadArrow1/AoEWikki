<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchTechnologyResultActivity">


    <TextView
        android:id="@+id/TextViewTechApply"
        android:layout_width="194dp"
        android:layout_height="160dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechBuildTime"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/TextViewTechBuildTime"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechCost" />

    <TextView
        android:id="@+id/TextViewTechAge"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechExpansion" />

    <TextView
        android:id="@+id/TextViewTechCost"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechBuilding" />

    <TextView
        android:id="@+id/TextViewTechBuilding"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechAge" />

    <TextView
        android:id="@+id/TextViewTechExpansion"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechDescription" />

    <TextView
        android:id="@+id/TextViewTechDescription"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:capitalize="none"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewTechName"
        app:layout_constraintVertical_bias="0.025" />

    <TextView
        android:id="@+id/TextViewTechName"
        android:layout_width="360dp"
        android:layout_height="61dp"
        android:layout_marginTop="32dp"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:scaleX="3"
        android:scaleY="3"
        android:shadowColor="@color/black"
        android:text="Name"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>