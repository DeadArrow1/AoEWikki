<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".SearchCivResultActivity">

    <TextView
        android:id="@+id/TextViewArmorBonus"
        android:layout_width="180dp"
        android:layout_height="140dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewAccuracy" />

    <TextView
        android:id="@+id/TextViewBlastRadius"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewSearchRadius" />

    <TextView
        android:id="@+id/TextViewSearchRadius"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewLineOfSight" />

    <TextView
        android:id="@+id/TextViewAttackBonus"
        android:layout_width="180dp"
        android:layout_height="140dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewMovementRate" />

    <TextView
        android:id="@+id/TextViewAccuracy"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewBlastRadius" />

    <TextView
        android:id="@+id/TextViewArmor"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewAttack" />

    <TextView
        android:id="@+id/TextViewAttack"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewRange" />

    <TextView
        android:id="@+id/TextViewRange"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewHitPoints" />

    <TextView
        android:id="@+id/TextViewHitPoints"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitDescription" />

    <TextView
        android:id="@+id/TextViewLineOfSight"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewArmor" />

    <TextView
        android:id="@+id/TextViewMovementRate"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewAttackDelay" />

    <TextView
        android:id="@+id/TextViewAttackDelay"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewReloadTime" />

    <TextView
        android:id="@+id/TextViewBuildTime"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitCost" />

    <TextView
        android:id="@+id/TextViewReloadTime"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewBuildTime" />

    <TextView
        android:id="@+id/TextViewUnitAge"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitExpansion" />

    <TextView
        android:id="@+id/TextViewUnitCost"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitBuilding" />

    <TextView
        android:id="@+id/TextViewUnitBuilding"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitAge" />

    <TextView
        android:id="@+id/TextViewUnitExpansion"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitDescription" />

    <TextView
        android:id="@+id/TextViewUnitDescription"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:capitalize="none"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextViewUnitName"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/TextViewUnitName"
        android:layout_width="360dp"
        android:layout_height="61dp"
        android:layout_marginTop="32dp"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:scaleX="3"
        android:scaleY="3"
        android:shadowColor="@color/black"
        android:text="Name"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>